{
  "id": "taurus",
  "label": "Taurus",
  "layout": "LayoutThemeA",
  "style": {
    "tokens": "taurus",
    "overrides": {
      "--spacing-small": "8px",
      "--spacing-medium": "16px",
      "--spacing-large": "24px",
      "--spacing-xlarge": "32px",
      "--background": "oklch(0.1591 0 0)",
      "--foreground": "oklch(1 0 0)",
      "--surface": "oklch(0.2603 0 0)",
      "--surface-foreground": "oklch(1 0 0)",
      "--primary-soft": "oklch(0.9253 0.0539 70.69)",
      "--primary": "oklch(0.8558 0.1095 69.85)",
      "--primary-hard": "oklch(0.7429 0.1802 56.67)",
      "--secondary-soft": "oklch(0.6223 0.1129 264.2)",
      "--secondary": "oklch(0.2759 0.0851 262.88)",
      "--secondary-hard": "oklch(0.2148 0.0698 262.72)"
    }
  },
  "layoutSlots": [
    {
      "name": "navbar",
      "component": "NavbarCompact",
      "props": {
        "logo": "Taurus",
        "items": [
          {
            "label": "Home",
            "href": "/"
          },
          {
            "label": "About",
            "href": "/about"
          },
          {
            "label": "Contact",
            "href": "/contact"
          }
        ]
      }
    }
  ],
  "pages": [
    {
      "name": "Home",
      "path": "/",
      "components": [
        {
          "section": "hero",
          "component": "HeroMinimal",
          "props": {
            "title": "Welcome to Taurus Theme",
            "subtitle": "A modern theme for your web application.",
            "ctaText": "Explore More",
            "ctaLink": "/explore"
          }
        },
        {
          "section": "home",
          "component": "HomeMinimal",
          "props": {
            "title": "Get Started Today",
            "description": "Join thousands of satisfied users"
          }
        },
        {
          "section": "home",
          "component": "HomeGrid",
          "props": {
            "title": "Our Features",
            "items": [
              {
                "title": "Fast Performance",
                "description": "Experience lightning-fast load times and smooth interactions."
              },
              {
                "title": "Responsive Design",
                "description": "Looks great on any device, from mobile to desktop."
              },
              {
                "title": "Customizable",
                "description": "Easily tailor the theme to fit your brand and style."
              }
            ],
            "columns": 3
          }
        }
      ]
    }
  ]
}