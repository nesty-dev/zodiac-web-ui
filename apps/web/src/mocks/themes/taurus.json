  "layoutSlots": [
    {
      "name": "navbar",
      "component": "NavbarCompact",
      "props": {
        "logo": "Aries",
        "items": [
          {
            "label": "Home",
            "href": "/"
          },
          {
            "label": "About",
            "href": "/about"
          },
          {
            "label": "Contact",
            "href": "/contact"
          }
        ]
      }
    }
  ],
  "pages": [
    {
